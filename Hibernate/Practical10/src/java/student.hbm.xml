<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Student" table="student">
        <id name="ID" type="long" column="Enrollment" >
            <generator class="assigned"/>
        </id>
        <property name="name">
            <column name="name"/>
        </property>
        <property name="sem">
            <column name="semester"/>
        </property>
        <property name="div">
            <column name="division"/>
        </property>
        <property name="dept">
            <column name="department"/>
        </property>
        <property name="sgpa">
            <column name="sgpa"/>
        </property>
        <property name="cgpa">
            <column name="cgpa"/>
        </property>
    </class>
</hibernate-mapping>